<tr>
  <th><%= t(".label", type: type.humanize) %></th>
  <td>
    <% if items.blank? %>
      <p><%= t('.empty', type: type.humanize) %></p>
    <% else %>
      <ul class="tabular">
        <% items.each do |item| %>
          <li class='attribute title'>
            <%= link_to item.title.first, [main_app, item] %>
          </li>
        <% end %>
      </ul>
    <% end %>
  </td>
</tr>
