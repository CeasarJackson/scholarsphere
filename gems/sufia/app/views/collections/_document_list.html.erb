<% @all_checked = false %>
<% @user = current_user %>
<% if documents.empty? %>
  <div class="alert alert-warning" role="alert"><%= t('sufia.collection.document_list.no_visible_works') %></div>
<% else %>
  <% @document_list = documents %>
  <% if params[:action] == "edit" %>
    <%= render partial:'my/_index_partials/default_group', locals:{documents: documents, docs:documents} %>
  <% else %>
    <%= render partial: 'show_document_list', locals:{documents: documents} %>
  <% end %>
<% end %>
